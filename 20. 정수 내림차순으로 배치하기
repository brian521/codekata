#include <string>
#include <vector>
#include <iostream>
#include <cmath>
#include <algorithm>
using namespace std;

long long solution(long long n) {
    long long answer = 0;
    vector<int> v;
    long long temp = n;
    while(temp > 0) {
        v.push_back(temp%10);
        temp /= 10;
    }
    sort(v.begin(), v.end());
    for(int i = 0; i < v.size(); i++) {
        answer += v[i]*pow(10, i);
    }
    return answer;
}
