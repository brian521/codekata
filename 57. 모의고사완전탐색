#include <string>
#include <vector>
#include <algorithm>

using namespace std;

vector<int> solution(vector<int> answers) {
    vector<int> answer;
    
    int correct1 = 0;
    int correct2 = 0;
    int correct3 = 0;
    
    for(int i = 0; i < answers.size(); i++)
    {
        if(i % 5 + 1 == answers[i]) correct1++;
        
        if(i % 2 == 0 && answers[i] == 2) correct2++;
        else if(i % 2 == 1)
        {
            if(i%8 == 1 && answers[i] == 1) correct2++;
            else if(i%8 == 3 && answers[i] == 3) correct2++;
            else if(i%8 == 5 && answers[i] == 4) correct2++;
            else if(i%8 == 7 && answers[i] == 5) correct2++;
        }
        
        if(i % 10 == 0 || i % 10 == 1)
        {
            if(answers[i] == 3) correct3++;
        }
        else if(i % 10 == 2 || i % 10 == 3)
        {
            if(answers[i] == 1) correct3++;
        }
        else if(i % 10 == 4 || i % 10 == 5)
        {   
            if(answers[i] == 2) correct3++;
        }
        else if(i % 10 == 6 || i % 10 == 7)
        {
            if(answers[i] == 4) correct3++;
        }
        else if(i % 10 == 8 || i % 10 == 9)
        {   
            if(answers[i] == 5) correct3++;
        }
    }
    
    int maxcor = max({correct1, correct2, correct3});
    
    if(maxcor == correct1) answer.push_back(1);
    if(maxcor == correct2) answer.push_back(2);
    if(maxcor == correct3) answer.push_back(3);
    
    return answer;
}
