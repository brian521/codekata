#include <string>
#include <vector>
#include <algorithm>
using namespace std;

vector<string> solution(vector<string> strings, int n) {
    vector<string> answer;
    vector<pair<char,int>> indexVec;
    
    sort(strings.begin(), strings.end());
    
    for(int i = 0; i < strings.size(); i++)
    {
        indexVec.push_back(make_pair(strings[i][n], i));
    }
    
    sort(indexVec.begin(), indexVec.end());
    
    for(int i = 0; i < indexVec.size(); i++)
    {
        answer.push_back(strings[indexVec[i].second]);
    }
    
    return answer;
}
