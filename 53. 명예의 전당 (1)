#include <string>
#include <vector>
#include <list>

using namespace std;

vector<int> solution(int k, vector<int> score) {
    vector<int> answer;
    list<int> k_score;

    for (int s : score) {
        auto it = k_score.begin();

        while (it != k_score.end() && *it >= s)
            ++it;

        k_score.insert(it, s);

        if (k_score.size() > k)
            k_score.pop_back();

        answer.push_back(k_score.back());
    }

    return answer;
}
