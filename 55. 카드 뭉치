#include <string>
#include <vector>

using namespace std;

string solution(vector<string> cards1, vector<string> cards2, vector<string> goal) {
    string answer = "";
    int mark1 = 0;
    int mark2 = 0;
    
    for(string word : goal)
    {
        if(word == cards1[mark1])
        {
            mark1++;
            continue;
        }
        else if(word == cards2[mark2])
        {
            mark2++;
            continue;
        }
        else
        {
            answer = "No";
            return answer;
        }
        
    }
    answer = "Yes";
    return answer;
}
