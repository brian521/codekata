#include <string>
#include <vector>

using namespace std;

string solution(vector<int> food) {
    string answer = "";
    string front = "";
    string back = "";
    
    for(int i = 1; i < food.size(); i++)
    {
        for(int j = 0; j < food[i] / 2; j++)
        {
            front = front + to_string(i);
            back = to_string(i) + back;
        }
    }
    answer = front + "0" + back;
    
    return answer;
}
