#include <string>
#include <vector>

using namespace std;

int solution(int number, int limit, int power) {
    int answer = 0;
    int yak = 0;
    
    for(int i = 1; i <= number; i++)
    {
        yak = 0;
        for(int j = 1; j*j <= i; j++)
        {
            if(j*j == i)
            {
                yak++;
            }
            else if(i%j == 0)
            {
                yak += 2;
            }
        }
        if(yak > limit)
        {
            yak = power;
        }
        answer += yak;
    }
    
    return answer;
}
