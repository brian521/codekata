#include <string>
#include <vector>

using namespace std;

string solution(int a, int b) {
    string answer = "";
    int days = 0;
    int total = b;
    for(int i = 1; i < a; i++)
    {
        if(i <= 7)
        {
            if(i == 2) days = 29;
            else if(i%2 == 1) days = 31;
            else days = 30;
        }
        else
        {
            if(i%2 == 1) days = 30;
            else days = 31;
        }
        total += days;
    }
    if(total%7 == 1) answer = "FRI";
    else if(total%7 == 2) answer = "SAT";
    else if(total%7 == 3) answer = "SUN";
    else if(total%7 == 4) answer = "MON";
    else if(total%7 == 5) answer = "TUE";
    else if(total%7 == 6) answer = "WED";
    else answer = "THU";
    return answer;
}
