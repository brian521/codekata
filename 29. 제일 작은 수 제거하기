#include <string>
#include <vector>

using namespace std;

vector<int> solution(vector<int> arr) {
    vector<int> answer;
    int* pMin = &arr[0];
    for(int i= 0; i < arr.size(); i++) {
        if(arr[i] < *pMin) {
            pMin = &arr[i];
        }
    }
    for(int i= 0; i < arr.size(); i++) {
        if(arr[i] != *pMin) {
            answer.push_back(arr[i]);
        }
    }
    if(answer.size() == 0) {
        answer.push_back(-1);
    }
    return answer;
}
